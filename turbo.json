{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "out/**"]
		},
		"check": {
			"dependsOn": ["^build"]
		},
		"ci:package": {},
		"ci:publish": {
			"passThroughEnv": ["VSCE_PAT", "GITHUB_TOKEN", "OVSX_PAT"]
		},
		"ci:update": {
			"outputs": ["packages/vscode-extension/src/apis.ts"]
		},
		"ci:version": {},
		"dev": {
			"cache": false
		},
		"test": {}
	}
}
