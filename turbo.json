{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "out/**"]
		},
		"dev": {
			"cache": false
		},
		"check": {
			"dependsOn": ["^build"]
		},
		"test": {},
		"ci:version": {},
		"ci:update": {
			"outputs": ["packages/vscode-extension/src/apis.ts"]
		}
	}
}
