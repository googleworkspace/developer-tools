{
	"$schema": "https://turbo.build/schema.json",
	"tasks": {
		"build": {
			"dependsOn": ["^build"],
			"outputs": ["dist/**", "out/**"]
		},
		"dev": {
			"cache": false
		},
		"check": {
			"dependsOn": ["^build"]
		},
		"test": {
			"outputs": [],
			"passThroughEnv": ["DISPLAY"]
		},
		"ci:version": {},
		"fetch-apis": {
			"outputs": ["packages/vscode-extension/src/apis.ts"]
		}
	}
}
